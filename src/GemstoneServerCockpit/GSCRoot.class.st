Class {
	#name : #GSCRoot,
	#superclass : #GSCTool,
	#instVars : [
		'config'
	],
	#category : #'GemstoneServerCockpit-Model'
}

{ #category : #'as yet unclassified' }
GSCRoot >> autogeneratedAppIndexFor: anApplicationTool [
	| dict |
	dict := self configAt: 'appIndexes' default: [ Dictionary new ].
	
	^ dict at: anApplicationTool shortCode ifAbsentPut: [
		| highest |
		highest := dict values ifEmpty: [ 0 ] ifNotEmpty: [ :all | all max ].
		(highest + 1) asInteger
	]
]

{ #category : #accessing }
GSCRoot >> config [

	^ config
]

{ #category : #accessing }
GSCRoot >> config: anObject [

	config := anObject
]

{ #category : #testing }
GSCRoot >> isRoot [
	^ true
]

{ #category : #accessing }
GSCRoot >> root [
	^ self
]
